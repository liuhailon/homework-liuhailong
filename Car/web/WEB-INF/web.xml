<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Car_Query</servlet-name>
        <servlet-class>servlet.Car_Query</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_Query</servlet-name>
        <url-pattern>/Car_Query</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Car_Add</servlet-name>
        <servlet-class>servlet.Car_Add</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_Add</servlet-name>
        <url-pattern>/Car_Add</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Car_Update</servlet-name>
        <servlet-class>servlet.Car_Update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_Update</servlet-name>
        <url-pattern>/Car_Update</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Car_Delete</servlet-name>
        <servlet-class>servlet.Car_Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_Delete</servlet-name>
        <url-pattern>/Car_Delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Car_UpdateCar</servlet-name>
        <servlet-class>servlet.Car_UpdateCar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_UpdateCar</servlet-name>
        <url-pattern>/Car_UpdateCar</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Car_QueryByname</servlet-name>
        <servlet-class>servlet.Car_QueryByname</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Car_QueryByname</servlet-name>
        <url-pattern>/Car_QueryByname</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>filter.EncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>